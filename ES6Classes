class Car {
  constructor(name) {
    this.brand = name;
  }
}

class Car {
  constructor(name) {
    this.brand = name;
  }
}
const mycar = new Car("Ford");

class Car {
  constructor(name) {
    this.brand = name;
  }
  present() {
    return 'I have a ' + this.brand;
  }
}
const mycar = new Car("Ford");
mycar.present();
